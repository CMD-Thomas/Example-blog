.category__header 
	margin-top: 0px
	height 200px 
	background-size: cover
	max-width: 100%
	h1
		color white
		font-size 36px
		text-align center
		padding-top 110px
		text-transform uppercase

.category__articles
	a,
	h2
		font-size 24px
		margin-top 20px
		text-decoration none
		font-family: 'Bitter', serif;
		font-weight 700
		color #333
		display: block
		line-height: 1.4em
		

.frontpage__titles
	font-size 24px
	margin-top 20px
	text-decoration none
	font-family: 'Bitter', serif;
	font-weight 700
	color #333
	display: block
	line-height: 1.4em
	&:hover
		color #333
		text-decoration none
		
.home--container
	padding-top 100px
	
.home__title
	text-transform uppercase
	color #333
	text-align center
	font-size 36px
	
.ic--date
	margin-right 10px

.article__information
	display: block
	margin-top: 20px
	margin-bottom 20px
	span 
		margin-left 10px
		display inline-block
		margin-right: 10px
	